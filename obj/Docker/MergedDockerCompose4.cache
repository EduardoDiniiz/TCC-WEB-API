networks:
  tcc-network:
    driver: bridge
services:
  devio.api:
    build:
      context: /Users/eduardolaurentino/Documents/Projetos/Dev/TCC.WEB.API
      dockerfile: src/DevIO.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DevIO.Api
      target: base
    container_name: DevIO.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      TZ: America/Sao_Paulo
    image: devio.api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/DevIO.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      tcc-network: {}
    ports:
    - published: 5000
      target: 80
    restart: always
    tty: true
    volumes:
    - /Users/eduardolaurentino/Documents/Projetos/Dev/TCC.WEB.API/src/DevIO.Api:/app:rw
    - /Users/eduardolaurentino/vsdbg/vs2017u5:/remote_debugger:rw
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/eduardolaurentino/.nuget/packages:/root/.nuget/packages:ro
    - /Users/eduardolaurentino/Documents/Projetos/Dev/TCC.WEB.API:/src:rw
version: '3.4'